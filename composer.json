{
    "name": "iishf/web2019",
    "description": "This is the IISHF Website Project",
    "type": "project",
    "homepage": "https://github.com/IISHF/web2019",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "stefan.gehrig.hn@googlemail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-PDO": "*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "babdev/pagerfanta-bundle": "^2.9",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/annotations": "^1.12",
        "doctrine/collections": "^1.6",
        "doctrine/common": "^3.1",
        "doctrine/dbal": "^2.12",
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/event-manager": "^1.1",
        "doctrine/migrations": "^3.1",
        "doctrine/orm": "^2.8",
        "doctrine/persistence": "^2.1",
        "exercise/htmlpurifier-bundle": "^3.1",
        "ezyang/htmlpurifier": "^4.13",
        "gedmo/doctrine-extensions": "^3.0",
        "giggsey/libphonenumber-for-php": "^8.12",
        "guzzlehttp/guzzle": "^6.5",
        "odolbeau/phone-number-bundle": "^3.3",
        "pagerfanta/doctrine-orm-adapter": "^2.7",
        "pagerfanta/pagerfanta": "^2.7",
        "phpdocumentor/reflection-docblock": "^5.2",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "psr/log": "^1.1",
        "ramsey/uuid": "^4.1",
        "sensio/framework-extra-bundle": "^6.1",
        "stof/doctrine-extensions-bundle": "^1.5",
        "symfony/asset": "5.2.*",
        "symfony/cache-contracts": "^2.2",
        "symfony/config": "5.2.*",
        "symfony/console": "5.2.*",
        "symfony/dependency-injection": "5.2.*",
        "symfony/dotenv": "5.2.*",
        "symfony/event-dispatcher": "5.2.*",
        "symfony/event-dispatcher-contracts": "^2.2",
        "symfony/expression-language": "5.2.*",
        "symfony/filesystem": "5.2.*",
        "symfony/flex": "^1.12",
        "symfony/form": "5.2.*",
        "symfony/framework-bundle": "5.2.*",
        "symfony/http-foundation": "5.2.*",
        "symfony/http-kernel": "5.2.*",
        "symfony/intl": "5.2.*",
        "symfony/mailer": "5.2.*",
        "symfony/messenger": "5.2.*",
        "symfony/mime": "5.2.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/options-resolver": "5.2.*",
        "symfony/process": "5.2.*",
        "symfony/property-access": "5.2.*",
        "symfony/property-info": "5.2.*",
        "symfony/routing": "5.2.*",
        "symfony/security-bundle": "5.2.*",
        "symfony/security-core": "5.2.*",
        "symfony/security-csrf": "5.2.*",
        "symfony/security-guard": "5.2.*",
        "symfony/security-http": "5.2.*",
        "symfony/serializer": "5.2.*",
        "symfony/translation": "5.2.*",
        "symfony/translation-contracts": "^2.3",
        "symfony/twig-bridge": "5.2.*",
        "symfony/twig-bundle": "5.2.*",
        "symfony/validator": "5.2.*",
        "symfony/web-link": "5.2.*",
        "symfony/webpack-encore-bundle": "^1.11",
        "symfony/workflow": "5.2.*",
        "symfony/yaml": "5.2.*",
        "twig/twig": "^3.3",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "5.2.*",
        "symfony/css-selector": "5.2.*",
        "symfony/debug-bundle": "5.2.*",
        "symfony/phpunit-bridge": "5.2.*",
        "symfony/stopwatch": "5.2.*",
        "symfony/var-dumper": "5.2.*",
        "symfony/web-profiler-bundle": "5.2.*"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
