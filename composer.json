{
    "name": "iishf/web2019",
    "description": "This is the IISHF Website Project",
    "type": "project",
    "homepage": "https://github.com/IISHF/web2019",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "stefan.gehrig.hn@googlemail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-PDO": "*",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "babdev/pagerfanta-bundle": "^2.4",
        "composer/package-versions-deprecated": "^1.10",
        "doctrine/annotations": "^1.0",
        "doctrine/collections": "^1.6",
        "doctrine/common": "^2.13",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^2.2",
        "doctrine/event-manager": "^1.1",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.7",
        "doctrine/persistence": "^1.3",
        "exercise/htmlpurifier-bundle": "^3.0",
        "ezyang/htmlpurifier": "^4.13",
        "gedmo/doctrine-extensions": "^2.4",
        "giggsey/libphonenumber-for-php": "^8.12",
        "guzzlehttp/guzzle": "^6.5",
        "odolbeau/phone-number-bundle": "^3.1",
        "pagerfanta/pagerfanta": "^2.3",
        "phpdocumentor/reflection-docblock": "^5.1",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "psr/log": "^1.1",
        "ramsey/uuid": "^4.0",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "^5.1",
        "symfony/cache-contracts": "^2.1",
        "symfony/config": "^5.1",
        "symfony/console": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/event-dispatcher": "^5.1",
        "symfony/event-dispatcher-contracts": "^2.1",
        "symfony/expression-language": "^5.1",
        "symfony/filesystem": "^5.1",
        "symfony/flex": "^1.8",
        "symfony/form": "^5.1",
        "symfony/framework-bundle": "^5.1",
        "symfony/http-foundation": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/intl": "^5.1",
        "symfony/mailer": "^5.1",
        "symfony/messenger": "^5.1",
        "symfony/mime": "^5.1",
        "symfony/monolog-bundle": "^3.5",
        "symfony/options-resolver": "^5.1",
        "symfony/process": "^5.1",
        "symfony/property-access": "^5.1",
        "symfony/property-info": "^5.1",
        "symfony/routing": "^5.1",
        "symfony/security-bundle": "^5.1",
        "symfony/security-core": "^5.1",
        "symfony/security-csrf": "^5.1",
        "symfony/security-guard": "^5.1",
        "symfony/security-http": "^5.1",
        "symfony/serializer": "^5.1",
        "symfony/translation": "^5.1",
        "symfony/translation-contracts": "^2.1",
        "symfony/twig-bridge": "^5.1",
        "symfony/twig-bundle": "^5.1",
        "symfony/validator": "^5.1",
        "symfony/web-link": "^5.1",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "^5.1",
        "symfony/yaml": "^5.1",
        "twig/twig": "^3.0",
        "webmozart/assert": "^1.9"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.5",
        "roave/security-advisories": "dev-master",
        "symfony/browser-kit": "^5.1",
        "symfony/css-selector": "^5.1",
        "symfony/debug-bundle": "^5.1",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/stopwatch": "^5.1",
        "symfony/var-dumper": "^5.1",
        "symfony/web-profiler-bundle": "^5.1"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^5.1"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
