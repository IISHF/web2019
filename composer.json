{
    "name": "iishf/web2019",
    "description": "This is the IISHF Website Project",
    "type": "project",
    "homepage": "https://github.com/IISHF/web2019",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Stefan Gehrig",
            "email": "stefan.gehrig.hn@googlemail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-ctype": "*",
        "ext-dom": "*",
        "ext-hash": "*",
        "ext-iconv": "*",
        "ext-imagick": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-PDO": "*",
        "ext-simplexml": "*",
        "ext-xml": "*",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "doctrine/collections": "^1.6",
        "doctrine/common": "^2.11",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/event-manager": "^1.1",
        "doctrine/migrations": "^2.2",
        "doctrine/orm": "^2.7",
        "doctrine/persistence": "^1.3",
        "exercise/htmlpurifier-bundle": "^3.0",
        "ezyang/htmlpurifier": "^4.12",
        "gedmo/doctrine-extensions": "^2.4",
        "giggsey/libphonenumber-for-php": "^8.11",
        "guzzlehttp/guzzle": "^6.4",
        "odolbeau/phone-number-bundle": "^3.0",
        "pagerfanta/pagerfanta": "^2.1",
        "psr/cache": "^1.0",
        "psr/container": "^1.0",
        "psr/log": "^1.1",
        "ramsey/uuid": "^3.9",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "^4.4",
        "symfony/cache-contracts": "^2.0",
        "symfony/config": "4.4.*",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "4.4.*",
        "symfony/dotenv": "^4.4",
        "symfony/event-dispatcher": "4.4.*",
        "symfony/event-dispatcher-contracts": "^1.1",
        "symfony/expression-language": "^4.4",
        "symfony/filesystem": "4.4.*",
        "symfony/flex": "^1.5",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4",
        "symfony/http-foundation": "4.4.*",
        "symfony/http-kernel": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/messenger": "^4.4",
        "symfony/mime": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/options-resolver": "4.4.*",
        "symfony/orm-pack": "^1.0",
        "symfony/process": "^4.4",
        "symfony/routing": "4.4.*",
        "symfony/security-bundle": "^4.4",
        "symfony/security-core": "4.4.*",
        "symfony/security-csrf": "4.4.*",
        "symfony/security-guard": "4.4.*",
        "symfony/security-http": "4.4.*",
        "symfony/serializer-pack": "^1.0",
        "symfony/translation-contracts": "^2.0",
        "symfony/twig-bridge": "4.4.*",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "^4.4",
        "symfony/web-link": "^4.4",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "^4.4",
        "symfony/yaml": "^4.4",
        "twig/twig": "^3.0",
        "webmozart/assert": "^1.6",
        "white-october/pagerfanta-bundle": "^1.3"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "phpunit/phpunit": "~8.4",
        "symfony/debug-pack": "^1.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "apcu-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    }
}
