{% extends 'internal.html.twig' %}

{% block content %}
    <h1>Create Member for Committee {{ committee.title }}</h1>

    {{ form_start(form) }}

    {{ form_rest(form) }}

    <div class="btn-group" role="group">
        <button type="submit" class="btn btn-success">Create</button>
        <a href="{{ path('app_committee_detail', {committee: committee.slug}) }}" class="btn btn-light">Cancel</a>
    </div>

    {{ form_end(form) }}
{% endblock %}
