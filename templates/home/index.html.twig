{% extends 'public_base.html.twig' %}

{% block body %}
    <h1>{{ number }}</h1>
    <a href="{{ path('app_admin_home_index') }}">Admin</a>,
    {% if app.user %}
        <a href="{{ logout_path() }}">Logout {{ app.user.username }}</a>
    {% else %}
        <a href="{{ path('login') }}">Login</a>
    {% endif %}
{% endblock %}
